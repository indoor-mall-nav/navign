{
  "identifier": "mobile-capability",
  "platforms": [
    "android",
    "iOS"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "biometric:default",
    "blec:default",
    "core:default",
    "http:default",
    "nfc:default",
    "geolocation:default",
    "geolocation:allow-check-permissions",
    "geolocation:allow-request-permissions",
    "geolocation:allow-get-current-position",
    "geolocation:allow-watch-position",
    "sql:default",
    "sql:allow-execute",
    "log:default",
    "store:default",
    "fs:default"
  ]
}